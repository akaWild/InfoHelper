<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:InfoHelper.Controls"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">
    <!--Actions panel-->
    <SolidColorBrush x:Key="ActionsPanelDefaultForegroundBrush">Black</SolidColorBrush>
    <SolidColorBrush x:Key="ActionsPanelBackgroundBrush">Beige</SolidColorBrush>

    <Style TargetType="controls:ActionsControl">
        <Setter Property="BorderBrush" Value="Black"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
    </Style>

    <!--Name panel-->
    <SolidColorBrush x:Key="NamePanelConfirmedForegroundBrush">Black</SolidColorBrush>
    <SolidColorBrush x:Key="NamePanelUnconfirmedForegroundBrush">Red</SolidColorBrush>
    <SolidColorBrush x:Key="NamePanelBackgroundBrush">Beige</SolidColorBrush>

    <Style x:Key="NamePanelBorderStyle" TargetType="Border">
        <Setter Property="BorderBrush" Value="Black"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
    </Style>

    <!--WindowInfos-->
    <SolidColorBrush x:Key="OkFrontWindowBackgroundBrush">LimeGreen</SolidColorBrush>
    <SolidColorBrush x:Key="OkBackWindowBackgroundBrush">#5032CD32</SolidColorBrush>
    <SolidColorBrush x:Key="WrongCaptionFrontWindowBackgroundBrush">Orange</SolidColorBrush>
    <SolidColorBrush x:Key="WrongCaptionBackWindowBackgroundBrush">#50FFA500</SolidColorBrush>
    <SolidColorBrush x:Key="ErrorFrontWindowBackgroundBrush">Red</SolidColorBrush>
    <SolidColorBrush x:Key="ErrorBackWindowBackgroundBrush">#50FF0000</SolidColorBrush>
    <Pen x:Key="WindowBorderPen" Brush="Black" Thickness="1"></Pen>
    <BitmapImage x:Key="HeroActingImage" UriSource="../Images/bell.png"></BitmapImage>
    <system:Double x:Key="HeroActingImageSizeRatio">0.2</system:Double>

    <!--AnalyzerInfo-->
    <SolidColorBrush x:Key="AnalyzerInfo">Green</SolidColorBrush>
    <SolidColorBrush x:Key="AnalyzerError">Red</SolidColorBrush>
    <SolidColorBrush x:Key="AnalyzerDefault">Black</SolidColorBrush>

    <!--SolverInfo-->
    <SolidColorBrush x:Key="SolverRunning">Blue</SolidColorBrush>
    <SolidColorBrush x:Key="SolverError">Red</SolidColorBrush>
    <SolidColorBrush x:Key="SolverDefault">Black</SolidColorBrush>

    <!--Gto control-->
    <SolidColorBrush x:Key="GtoHeaderForegroundBrush">Black</SolidColorBrush>
    <SolidColorBrush x:Key="GtoHeaderBackgroundBrush">BurlyWood</SolidColorBrush>
    <SolidColorBrush x:Key="GtoForegroundBrush">Black</SolidColorBrush>
    <Pen x:Key="GtoBorderPen" Brush="Black" Thickness="1"></Pen>
    <Pen x:Key="GtoSelectedHandBorderPen" Brush="Blue" Thickness="2"></Pen>
    <SolidColorBrush x:Key="GtoFoldBrush">#FF5956</SolidColorBrush>
    <SolidColorBrush x:Key="GtoCheckCallBrush">#FFFF33</SolidColorBrush>

    <x:Array x:Key="GtoRaiseBrushes" Type="SolidColorBrush">
        <SolidColorBrush>#8FFF8F</SolidColorBrush>
        <SolidColorBrush>#28CC28</SolidColorBrush>
        <SolidColorBrush>LightSkyBlue</SolidColorBrush>
        <SolidColorBrush>#006DFF</SolidColorBrush>
    </x:Array>

    <x:Array x:Key="GtoDiffBrushes" Type="SolidColorBrush">
        <SolidColorBrush>#A9FEAD</SolidColorBrush>
